- content_for(:leftcolumn) do
  .centered
    = link_to "+", new_manifest_path, class: 'btn-add fixed', title: 'Add Manifest'
.row.grid_sm
  .col
    %table.table
      %tr
        %th.tableheader{colspan:100} Device Models
      %tr
        %th Name
        %th Version
      - @device_models.each do |device_model|
        %tr
          %td= link_to device_model.name, manifest_path(device_model.current_manifest)
          %td= link_to device_model.current_manifest.version, manifest_path(device_model.current_manifest)

