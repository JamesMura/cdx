.row-fluid
  .span12
    %p#notice= notice
    %p
      %strong Version:
      = @manifest.version
    %p
      %strong Definition:
      %pre= JSON.pretty_generate(JSON.parse(@manifest.definition))

    = link_to 'Back', manifests_path, :class => 'btn'
    = link_to 'Delete', manifest_path(@manifest), method: "DELETE", confirm: "Are you sure?", :class => 'btn btn-link'

