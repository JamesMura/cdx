#show-dst-lpa-result.testflow
  .container
    .row.labelHeader
      .col-6
        %h3
          = I18n.t('dst_lpa_results.show.header', patient_name: patient_display_name(@requested_test.encounter.patient.name))
      .col-6
        %ul
          %li
            = link_to I18n.t('views.back'), encounter_path(@requested_test.encounter, :test_order_page_mode => @return_page_mode), class: 'button new'
    .order-details
      .panel
        .row
          .col-4
            .row
              .col-6
                %label
                  = DstLpaResult.human_attribute_name(:sample_collected_on)
              .col-6
                = I18n.l(@dst_lpa_result.sample_collected_on, format: :full_date)
            .row
              .col-6
                %label
                  = DstLpaResult.human_attribute_name(:media_used)
              .col-6
                = Extras::Select.find(DstLpaResult.method_options, @dst_lpa_result.media_used)
            .row
              .col-6
                %label
                  = DstLpaResult.human_attribute_name(:serial_number)
              .col-6
                = @dst_lpa_result.serial_number
            .row
              .col-6
                %label
                  = DstLpaResult.human_attribute_name(:examined_by)
              .col-6
                = @dst_lpa_result.examined_by
            .row
              .col-6
                %label
                  = DstLpaResult.human_attribute_name(:result_on)
              .col-6
                = I18n.l(@dst_lpa_result.result_on, format: :full_date)

          .col-4
            .row
              .col-6
                %label
                  = DstLpaResult.human_attribute_name(:results_h)
              .col-6
                = Extras::Select.find(DstLpaResult.dst_lpa_options, @dst_lpa_result.results_h)
