{
  "metadata": {
    "version": "1.0.94",
    "api_version": "1.1.0",
    "device_models": [
      "GX Model I"
    ],
    "conditions": [
      "MTB"
    ],
    "source": {
      "type": "json"
    }
  },
  "field_mapping": {
    "patient": [
      {
        "target_field": "age",
        "source": {
          "lookup": "event.age"
        },
        "type": "integer",
        "core": true,
        "indexed": true,
        "valid_values": {
          "range": {
            "min": 0,
            "max": 125
          }
        }
      },
      {
        "target_field": "gender",
        "source": { "case" : [
          { "lookup": "event.gender" },
          [
            { "when" : "M", "then" : "male"},
            { "when" : "F", "then" : "female"},
            { "when" : "*", "then" : "unknown"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options": [
          "male",
          "female",
          "unknown"
        ]
      },
      {
        "target_field": "ethnicity",
        "source": { "case" : [
          { "lookup": "event.ethnicity" },
          [
            { "when" : "hispanic", "then" : "hispanic"},
            { "when" : "not_hispanic", "then" : "not_hispanic"},
            { "when" : "*", "then" : "unknown"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options" : [
          "hispanic",
          "not_hispanic",
          "unknown"
        ]
      },
      {
        "target_field": "race",
        "source": { "case" : [
          {"lookup": "event.race"},
          [
            { "when" : "2054-5", "then" : "black_or_african_american"},
            { "when" : "1002-5", "then" : "american_indian_or_alaska_native"},
            { "when" : "2028-9", "then" : "asian"},
            { "when" : "2076-8", "then" : "native_hawaiian_or_other_pacific_islander"},
            { "when" : "2106-3", "then" : "white"},
            { "when" : "2131-1", "then" : "other"},
            { "when" : "*", "then" : "unknown"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options" : [
          "black_or_african_american",
          "american_indian_or_alaska_native",
          "asian",
          "native_hawaiian_or_other_pacific_islander",
          "white",
          "other",
          "unknown"
        ]
      },
      {
        "target_field": "race_ethnicity",
        "source": { "case" : [
          {"lookup": "event.race_ethnicity"},
          [
            { "when" : "2135-2", "then" : "hispanic"},
            { "when" : "2054-5", "then" : "black_or_african_american"},
            { "when" : "1002-5", "then" : "american_indian_or_alaska_native"},
            { "when" : "2028-9", "then" : "asian"},
            { "when" : "2076-8", "then" : "native_hawaiian_or_other_pacific_islander"},
            { "when" : "2106-3", "then" : "white"},
            { "when" : "2131-1", "then" : "other"},
            { "when" : "*", "then" : "unknown"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options" : [
          "hispanic",
          "black_or_african_american",
          "american_indian_or_alaska_native",
          "asian",
          "native_hawaiian_or_other_pacific_islander",
          "white",
          "other",
          "unknown"
        ]
      }
    ],
    "sample": [
      {
        "target_field": "sample_uid",
        "source": {
          "lookup": "event.sample_ID"
        },
        "type": "string",
        "core": true,
        "pii": true
      },
      {
        "target_field": "sample_id",
        "source": {
          "lookup": "event.sample_ID"
        },
        "type": "string",
        "core": true,
        "indexed": true
      }
    ],
    "test": [
      {
        "target_field": "start_time",
        "source": {
          "lookup": "event.start_time"
        },
        "type": "date",
        "core": true,
        "indexed": true
      },
      {
        "target_field": "end_time",
        "source": {
          "lookup": "event.end_time"
        },
        "type": "date"
      },
      {
        "target_field": "test_id",
        "source": {
          "lookup": "event.event_id"
        },
        "type": "string",
        "core": true,
        "indexed": true
      },
      {
        "target_field": "system_user",
        "source": {
          "lookup": "event.system_user"
        },
        "type": "string",
        "core": true,
        "indexed": true
      },
      {
        "target_field": "device_serial_number",
        "source": {
          "lookup": "event.device_serial_number"
        },
        "type": "string",
        "core": true,
        "indexed": true
      },
      {
        "target_field": "error_code",
        "source": {
          "lookup": "event.error_code"
        },
        "type": "integer",
        "core": true,
        "indexed": true
      },
      {
        "target_field": "error_description",
        "source": {
          "lookup": "event.error_description"
        },
        "type": "string",
        "core": true,
        "indexed": true
      },
      {
        "target_field": "assay_name",
        "source": {
          "lookup": "event.assay_name"
        },
        "type": "string",
        "core": true,
        "indexed": true,
        "options": [
          "MTB Assay",
          "Xpert SA Nasal Complete G3",
          "MTB-RIF Assay G4_noQC1_2"
        ]
      },
      {
        "target_field": "status",
        "source": { "case" : [
          { "lookup": "event.result" },
          [{"when" : "INVALID", "then" : "invalid" },
            {"when" : "ERROR", "then" : "error" },
            {"when" : "NO RESULT", "then" : "no_result" },
            {"when" : "MTB NOT DETECTED", "then" : "negative" },
            {"when" : "Rif Resistance DETECTED", "then" : "success" },
            {"when" : "Rif Resistance INDETERMINATE", "then" : "success" },
            {"when" : "MTB DETECTED", "then" : "success" },
            {"when" : "*", "then" : "error"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options": [
          "invalid",
          "error",
          "no_result",
          "success",
          "negative"
        ]
      },
      {
        "target_field": "results[*].result",
        "source": { "case" : [
          { "lookup": "event.result" },
          [ {"when" : "INVALID", "then" : "unknown" },
            {"when" : "ERROR", "then" : "unknown" },
            {"when" : "NO RESULT", "then" : "unknown" },
            {"when" : "MTB NOT DETECTED", "then" : "negative" },
            {"when" : "Rif Resistance DETECTED", "then" : "positive_with_rif" },
            {"when" : "Rif Resistance INDETERMINATE", "then" : "positive_with_rif_inconclusive" },
            {"when" : "MTB DETECTED", "then" : "positive" },
            {"when" : "*", "then" : "unknown"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options": [
          "unknown",
          "positive",
          "negative",
          "positive_with_rif",
          "positive_with_rif_inconclusive"
        ]
      },
      {
        "target_field": "results[*].condition",
        "source": { "case" : [
          { "lookup" : "event.assay_name"},
          [ {"when" : "Xpert MTB-RIF", "then" : "mtb"},
            {"when" : "Xpert MTB-RIF G3", "then" : "mtb"},
            {"when" : "MTB-RIF Assay G4", "then" : "mtb"},
            {"when" : "Xpert MTB-RIF Assay G4", "then" : "mtb"},
            {"when" : "MTB Beta", "then" : "mtb"},
            {"when" : "*", "then" : "mtb"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options": [
          "mtb"
        ]
      },
      {
        "target_field": "test_type",
        "source": { "case" : [
          { "lookup": "event.test_type" },
          [
            { "when" : "0", "then" : "specimen"},
            { "when" : "*", "then" : "qc"}
          ]
        ]},
        "type": "enum",
        "core": true,
        "indexed": true,
        "options": [
          "specimen",
          "qc"
        ]
      }
    ]
  }
}
