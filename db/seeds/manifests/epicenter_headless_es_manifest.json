{
  "metadata": {
    "version": "0.0.4",
    "api_version": "1.2.1",
    "signature": "",
    "device_models": [
      "epicenter_headless_es"
    ],
    "conditions": [
      "MTB"
    ],
    "source": {
      "type": "headless_csv",
      "separator": ","
    }
  },
  "custom_fields": {
    "patient.name": {
      "pii" : true
    }
  },
  "field_mapping": {
    "patient.id": { "lookup": "4" },
    "patient.name": { "lookup": "3" },
    "sample.id": { "lookup": "0" },
    "sample.uid": {
      "concat" : [
          {"lookup" : "4"},
          "-",
          {"lookup" : "0"}
      ]
    },
    "test.patient_age": {
      "if" : [
        {"equals" : [{"lookup" : "19"}, null]},
        "",
        {
          "years_between" : [
            {"parse_date" : [
              {"lookup" : "19"},
              "%Y-%m-%d %H:%M:%S.%L"
            ]},
            {"parse_date" : [
              {"lookup" : "13"},
              "%Y-%m-%d %H:%M:%S.%L"
            ]}
          ]
        }
      ]
    },
    "test.id": {
      "concat" : [
          {"lookup" : "0"},
          "-",
          {"lookup" : "9"},
          "-",
          {"lookup" : "1"},
          "-",
          {"lookup" : "2"}
      ]
    },
    "test.start_time": {
      "parse_date": [
        {
          "lookup": "13"
        },
        "%Y-%m-%d %H:%M:%S.%L"
      ]
    },
    "test.end_time": {
      "parse_date": [
        {
          "lookup": "12"
        },
        "%Y-%m-%d %H:%M:%S.%L"
      ]
    },
    "test.name": { "lookup": "7" },
    "test.assays[*].name": {
      "if" : [
        {"equals" : [{"lookup" : "11"}, "AST - MGIT"]},
        {
          "case": [
          {"lookup" : "14"},
          [
            {"when": "Estreptomicina", "then" : "streptomycin"},
            {"when": "Etambutol", "then" : "ethambutol"},
            {"when": "Rifampicina", "then" : "rifampicin"},
            {"when": "Pirazinamida", "then" : "pyrazinamide"},
            {"when": "Isoniazid", "then" : "isoniazid"},
            {"when": "*cido p-Aminosalic*lico", "then" : "p_aminosalicyclic_acid"},
            {"when": "Hidrazida del *cido tiofenocarbox*lico", "then" : "thiophenecarboxylic_acid_hydrazide"},
            {"when": "Levofloxacino", "then" : "levofloxacin"},
            {"when": "Tiosemicarbazona", "then" : "thiosemicarbazone"},
            {"when": "*", "then" : "mtb"}
          ]
        ]},
        "mtb"
      ]
    },
    "test.assays[*].qualitative_result": {
      "if" : [
        {"equals" : [{"lookup" : "11"}, "AST - MGIT"]},
        {
          "case": [
          {"lookup" : "15"},
          [
            {"when": "R", "then" : "positive"},
            {"when": "S", "then" : "negative"},
            {"when": "*", "then" : "n/a"}
          ]
        ]},
        {
          "case" : [
            {"lookup" : "9"},
            [
              {"when": "Positivo", "then" : "positive"},
              {"when": "Negativo", "then" : "negative"},
              {"when": "*", "then" : "n/a"}
            ]
          ]
        }
      ]
    },
    "test.status": {
      "if" : [
        {"equals" : [{"lookup" : "11"}, "AST - MGIT"]},
        {
          "case": [
          {"lookup" : "15"},
          [
            {"when": "S", "then" : "success"},
            {"when": "R", "then" : "success"},
            {"when": "*", "then" : "invalid"}
          ]
        ]},
        {
          "case" : [
            { "lookup": "9" },
            [
              {"when" : "Positivo", "then" : "success" },
              {"when" : "Completo retirado", "then" : "success" },
              {"when" : "Negativo", "then" : "success" },
              {"when" : "No v√°lido", "then" : "invalid" },
              {"when" : "En curso retirado", "then" : "error" },
              {"when" : "Umbral positivo", "then" : "no_result" },
              {"when" : "*", "then" : "unknown"}
            ]
          ]
        }
      ]
    },
    "test.type": "specimen"
  }
}
