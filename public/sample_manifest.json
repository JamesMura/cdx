{
  "metadata" : {
    "version" : "1.0.0",
    "signature" : "12345",
    "device_models" : ["GX4001", "GX4002"],
    "source_data_type": "json"
  },
  "field_mapping" : [
    {
    	"target_field": "patient_name",
      "selector" : "patient_information.name",
      "type" : "core"
    },
    {
      "target_field": "age",
      "type": "core",
      "selector" : "patient_information.age",
      "transform" : {
        "days_in" : ["month"],

        // "Condition": {"Bool": {"aws:MultiFactorAuthPresent": "true"}},
        // "Condition": {"Month"},
        // "Condition": {"StringEquals":{ "s3:prefix" : ["","home/"] , "s3:delimiter" : ["/"] }},
        // "*MTB*" : "MTB",
        // "*FLU*" : "H1N1",
        // "*FLUA*" : "A1N1"
      },
      "valid_values": {
        "range" : {
          "min" : 0,
          "max" : 100
        }
      }
    },
    {
    	"target_field": "assay_name",
      "selector" : "Test.assay_name",
      "type" : "core"
    },
    {
    	"target_field": "system_user",
      "selector" : "doctor_name",
      "type": "custom"
    },
    {
    	"target_field": "results[*].conditions",
    	"type": "core",
      "multiple" : true,
      "selector" : "conditions[*].condition",
      "value_mappings" : {
        "*MTB*" : "MTB",
        "*FLU*" : "H1N1",
        "*FLUA*" : "A1N1"
      }
    },
    {
    	"target_field": "results[*].result",
    	"type": "core",
      "selector" : "conditions[*].condition",
      "value_mappings" : {
        "*NOT DETECTED*" : "negative",
        "*POSITIVE*" : "positive",
        "*POSITIVE WITH RIF*" : "positive with rif"
      }
    },
    {
     	"target_field": "grade_of_resistance",
     	"type": "custom",
      "selector" : "resistance",
      "valid_values": {
        "range" : {
          "min" : 0,
          "max" : 100
        }
      }
    },
    {
    	"target_field": "device_serial_number",
    	"type": "core",
      "selector" : "patient_information.name"
    },
    {
    	"target_field": "test_type",
      "selector" : "Test.test_type",
      "type": "custom",
      "valid_values" : {
        "options" : [0, 1]
      }
    },
    {
    	"target_field": "temperature",
    	"type": "custom",
      "selector" : "Test.test_type",
      "valid_values": {
        "range" : {
          "min" : -20,
          "max" : 60
        }
      },
      "indexed" : true
    },
    {
    	"target_field": "percentage_of_confidence",
    	"type": "custom",
      "selector" : "Test.test_type",
      "valid_values": {
        "range" : {
          "min" : 0.001,
          "max" : 999.999
        }
      },
      "indexed" : true
    },
    {
    	"target_field": "end_time",
    	"type": "custom",
      "selector" : "Test.end_time",
      "valid_values": {
        "date" : "iso"
      },
      "indexed" : false
    },
    {
    	"target_field": "result_text",
    	"type": "custom",
      "selector" : "assay.result_text",
      "data-type" : "string",
      "pii" : false,
      "indexed" : true
    }
  ]
}
